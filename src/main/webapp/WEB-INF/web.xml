<?xml version="1.0" encoding="UTF-8"?>
<javaee:web-app version="3.0" xmlns:javaee="http://java.sun.com/xml/ns/javaee"
 xmlns:xml="http://www.w3.org/XML/1998/namespace" 
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_0.xsd ">
<javaee:display-name>osgi-web-app</javaee:display-name>
<javaee:welcome-file-list>
	<javaee:welcome-file>pages/index.jsp</javaee:welcome-file>
</javaee:welcome-file-list>
<javaee:servlet>
	<javaee:servlet-name>osgi-web-app</javaee:servlet-name>
	<javaee:servlet-class>presentation.Controller</javaee:servlet-class>
</javaee:servlet>
<javaee:servlet-mapping>
	<javaee:servlet-name>osgi-web-app</javaee:servlet-name>
	<javaee:url-pattern>/get-info/*</javaee:url-pattern>
</javaee:servlet-mapping>


</javaee:web-app>
